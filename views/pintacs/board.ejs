<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Board</title>
</head>
<body>
  <h1>Board: <%= board.name %></h1>

  <% pins.forEach(function(pin) { %>
    <div class="pin">
      <h2><%= pin.title %></h2>
      <p><%= pin.description %></p>
      <img src="<%= pin.image %>">
    </div>
  <% }); %>
</body>
</html>
